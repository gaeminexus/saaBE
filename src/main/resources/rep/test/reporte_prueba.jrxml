<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd"
              name="reporte_prueba" pageWidth="612" pageHeight="792" columnWidth="555" leftMargin="20" rightMargin="20" topMargin="20" bottomMargin="20">
    
    <property name="ireport.zoom" value="1.0"/>
    <property name="ireport.x" value="0"/>
    <property name="ireport.y" value="0"/>
    
    <parameter name="empresaId" class="java.lang.Integer">
        <defaultValueExpression><![CDATA[1]]></defaultValueExpression>
    </parameter>
    
    <parameter name="fechaDesde" class="java.lang.String">
        <defaultValueExpression><![CDATA["2026-01-01"]]></defaultValueExpression>
    </parameter>
    
    <parameter name="fechaHasta" class="java.lang.String">
        <defaultValueExpression><![CDATA["2026-01-31"]]></defaultValueExpression>
    </parameter>
    
    <parameter name="RUTA_IMAGENES" class="java.lang.String"/>
    
    <title>
        <band height="79" splitType="Stretch">
            <staticText>
                <reportElement x="0" y="10" width="572" height="30"/>
                <textElement textAlignment="Center">
                    <font size="20" isBold="true"/>
                </textElement>
                <text><![CDATA[Reporte de Prueba - Test]]></text>
            </staticText>
            <staticText>
                <reportElement x="0" y="45" width="572" height="20"/>
                <textElement textAlignment="Center">
                    <font size="12"/>
                </textElement>
                <text><![CDATA[Sistema Administrativo y Contable]]></text>
            </staticText>
        </band>
    </title>
    
    <pageHeader>
        <band height="35" splitType="Stretch">
            <textField>
                <reportElement x="0" y="0" width="200" height="15"/>
                <textElement>
                    <font size="10"/>
                </textElement>
                <textFieldExpression><![CDATA["Empresa ID: " + $P{empresaId}]]></textFieldExpression>
            </textField>
            <textField>
                <reportElement x="0" y="15" width="200" height="15"/>
                <textElement>
                    <font size="10"/>
                </textElement>
                <textFieldExpression><![CDATA["Período: " + $P{fechaDesde} + " - " + $P{fechaHasta}]]></textFieldExpression>
            </textField>
            <textField pattern="dd/MM/yyyy HH:mm">
                <reportElement x="400" y="0" width="172" height="15"/>
                <textElement textAlignment="Right">
                    <font size="10"/>
                </textElement>
                <textFieldExpression><![CDATA[new java.util.Date()]]></textFieldExpression>
            </textField>
        </band>
    </pageHeader>
    
    <detail>
        <band height="125" splitType="Stretch">
            <staticText>
                <reportElement x="0" y="10" width="572" height="100"/>
                <box>
                    <pen lineWidth="1.0"/>
                </box>
                <textElement textAlignment="Center" verticalAlignment="Middle">
                    <font size="14"/>
                </textElement>
                <text><![CDATA[Este es un reporte de prueba.
                
Si ves este mensaje, significa que el sistema de reportes 
está funcionando correctamente.

Parámetros recibidos correctamente.]]></text>
            </staticText>
        </band>
    </detail>
    
    <pageFooter>
        <band height="54" splitType="Stretch">
            <textField>
                <reportElement x="0" y="20" width="100" height="20"/>
                <textElement>
                    <font size="10"/>
                </textElement>
                <textFieldExpression><![CDATA["Página " + $V{PAGE_NUMBER}]]></textFieldExpression>
            </textField>
        </band>
    </pageFooter>
    
</jasperReport>

	<property name="com.jaspersoft.studio.data.defaultdataadapter" value="saa_oracle_xe"/>
	<query language="SQL"><![CDATA[SELECT 
    ROW_NUMBER() OVER (ORDER BY NOMBRE) AS no,
    codigo,
    rol,
    NOMBRE,
    CASE 
        WHEN jubilacion_personal = 0 AND cesantia_personal > 0
            THEN cesantia_personal + NVL(aportes_ah,0)
        WHEN jubilacion_personal > 0 AND cesantia_personal = 0
            THEN cesantia_personal
        WHEN jubilacion_personal > 0 AND cesantia_personal > 0
            THEN cesantia_personal + ROUND(NVL(aportes_ah,0) * 0.3, 2)
        ELSE cesantia_personal
    END AS cesantia_personal,
    CASE 
        WHEN jubilacion_personal = 0 AND cesantia_personal > 0
            THEN jubilacion_personal
        WHEN jubilacion_personal > 0 AND cesantia_personal = 0
            THEN jubilacion_personal + NVL(aportes_ah,0)
        WHEN jubilacion_personal > 0 AND cesantia_personal > 0
            THEN jubilacion_personal + ROUND(NVL(aportes_ah,0) * 0.7, 2)
        ELSE jubilacion_personal
    END AS jubilacion_personal,
    cesantia_retiro_voluntario,
    jubilacion_retiro_voluntario,
    jubilacion_patronal,
    cesantia_patronal,
    pension_complementaria,
    (
        CASE 
            WHEN jubilacion_personal = 0 AND cesantia_personal > 0
                THEN NVL(cesantia_personal + NVL(aportes_ah,0), 0)
            WHEN jubilacion_personal > 0 AND cesantia_personal = 0
                THEN NVL(cesantia_personal, 0)
            WHEN jubilacion_personal > 0 AND cesantia_personal > 0
                THEN NVL(cesantia_personal + ROUND(NVL(aportes_ah,0) * 0.3, 2),0)
            ELSE NVL(cesantia_personal,0)
        END
        +
        CASE 
            WHEN jubilacion_personal = 0 AND cesantia_personal > 0
                THEN NVL(jubilacion_personal,0)
            WHEN jubilacion_personal > 0 AND cesantia_personal = 0
                THEN NVL(jubilacion_personal + NVL(aportes_ah,0),0)
            WHEN jubilacion_personal > 0 AND cesantia_personal > 0
                THEN NVL(jubilacion_personal + ROUND(NVL(aportes_ah,0) * 0.7, 2),0)
            ELSE NVL(jubilacion_personal,0)
        END
        + NVL(cesantia_retiro_voluntario,0)
        + NVL(jubilacion_retiro_voluntario,0)
        + NVL(jubilacion_patronal,0)
        + NVL(cesantia_patronal,0)
        + NVL(pension_complementaria,0)
    ) AS total_cuentas
FROM (
    SELECT 
        e.ENTDNMID AS codigo,
        e.ENTDRLPC AS rol,
        e.ENTDRZNS AS NOMBRE,
        a.TPAPCDGO,
        a.APRTVLRR,
        (SELECT SUM(p.PXCADSDO) 
         FROM crd.pxca p
         JOIN crd.dtca d ON p.dtcacdgo = d.dtcacdgo
         WHERE p.pxcacdpt = e.ENTDRLPC
           AND d.DTCACDPP = 'AH') AS aportes_ah
    FROM crd.entd e
    JOIN crd.aprt a 
        ON e.entdcdgo = a.entdcdgo
    WHERE e.ENTDRLPC IS NOT NULL
) 
PIVOT (
    SUM(APRTVLRR)
    FOR TPAPCDGO IN (
        11 AS cesantia_personal,
        9  AS jubilacion_personal,
        21 AS cesantia_retiro_voluntario,
        22 AS jubilacion_retiro_voluntario,
        13 AS jubilacion_patronal,
        14 AS cesantia_patronal,
        23 AS pension_complementaria
    )
)
ORDER BY NOMBRE;]]></query>
